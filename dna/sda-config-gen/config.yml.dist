      fusion1:
        <<: *fusion1
        port: Fo1/0/1
      fusion2:
        <<: *fusion2
        port: Fo1/0/2
      fexit:
        <<: *fexit
      fs1a:
        port: Fo1/0/4
        linknet4: 10.14.1.0/31
      fs1b:
        port: Fo1/0/5
