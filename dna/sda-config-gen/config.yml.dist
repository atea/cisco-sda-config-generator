      fusion1: &fusion1
        port: Fo1/0/1
        native: '999'
        allowed: 200-299
      fusion2: &fusion2
        port: Fo1/0/2
        native: '999'
        allowed: 300-399
      fexit: &fexit
        port1:
          port:
            fexit1: Fo1/0/3
            fexit2: Fo1/0/3
          linknet4: 10.13.1.0/31
        port2:
          port:
            fexit1: Fo1/0/4
            fexit2: Fo1/0/4
          linknet4: 10.13.2.0/31
